<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<template id="theme_common_footer_eleven" inherit_id="website.layout" name="Footer Eleven" active="False">
       	<xpath expr="//div[@id='footer']" position="replace">
       		<div id="footer" class="oe_structure oe_structure_solo" t-ignore="true" t-if="not no_footer">
				<section id="footer_one" class="footer_one o_footer">
					<section class="top-footer pt64 pb64 bg-white">
						<div class="container">
							<div class="newsletter-content text-center">
								<h3 class="mb-3">Our Newsletter</h3>
								<p class="mb-3">Join our list and get 20% OFF your first purchase! Don’t worry we don’t spam</p>
								<div class="block-subscribe-footer w-lg-75 mx-auto">
									<form class="s_newsletter_subscribe_form s_newsletter_list js_subscribe" data-vxml="001" data-list-id="2">
										<div class="js_subscribed_wrap d-none">
											<p class="h4-fs text-center text-success js_subscribed_btn"><i class="fa fa-check-circle-o" role="img"/>Thanks for registering!</p>
										</div>
										<div class="js_subscribe_wrap">
											<div class="input-group">
												<input type="email" name="email" class="js_subscribe_value form-control bg-white" placeholder="Enter Your Email"/>
												<span class="input-group-append">
													<button role="button" class="btn btn-primary js_subscribe_btn h-100 o_submit">Subscribe</button>
												</span>
											</div>
										</div>
									</form>
								</div>
							</div>
						</div>
					</section>
					<section t-if="not no_copyright" class="o_footer_copyright  o_cc pb24">
                        <div class="container-fluid">
                            <div class="row justify-content-center">
                            	<div class="col-lg-4 col-xl-3 mb-4 mb-lg-0">
									<div class="footer-right text-center text-lg-start text-dark">
										<p class="m-0">
											<span>© 2021 All rights reserved.</span>
											<span t-field="res_company.name" />
										</p>
									</div>
								</div>
                                <div class="col-lg-4 mb-4 mb-lg-0">
									<div class="footer-links">
										<ul class="nav justify-content-center">
											<li class="nav-item">
												<a href="#" class="nav-link ">Home</a>
											</li>
											<li class="nav-item">
												<a href="#" class="nav-link ">Contact Us</a>
											</li>
											<li class="nav-item">
												<a href="#" class="nav-link ">About Us</a>
											</li>
											<li class="nav-item">
												<a href="#" class="nav-link ">Policy</a>
											</li>
											<li class="nav-item">
												<a href="#" class="nav-link ">FAQ</a>
											</li>
										</ul>
									</div>
								</div>
								<div class="col-lg-4 col-xl-3">
									<div class="footer-middle text-center text-lg-end">
										<div class="s_social_media o_not_editable" data-snippet="s_social_media" data-name="Social Media" contenteditable="false">
											<a href="/website/social/facebook" class="link ms-4 s_social_media_facebook" target="_blank" aria-label="Facebook">
												<i class="fa fa-facebook o_editable_media"/>
											</a>
											<a href="/website/social/twitter" class="link ms-4 s_social_media_twitter" target="_blank" aria-label="X">
												<i class="fa fa-twitter o_editable_media"/>
											</a>
											<a href="/website/social/linkedin" class="link ms-4 s_social_media_linkedin" target="_blank" aria-label="LinkedIn">
												<i class="fa fa-linkedin o_editable_media"/>
											</a>
											<a href="/website/social/youtube" class="link ms-4 s_social_media_youtube" target="_blank" aria-label="Youtube">
												<i class="fa fa-youtube-play o_editable_media"/>
											</a>
											<a href="/website/social/instagram" class="link ms-4 s_social_media_instagram" target="_blank" aria-label="Instagram">
												<i class="fa fa-instagram o_editable_media"/>
											</a>
											<a href="/website/social/github" class="link ms-4 s_social_media_github" target="_blank" aria-label="Github">
												<i class="fa fa-github o_editable_media"/>
											</a>
										</div>
									</div>
								</div>
								
                            </div>
                        </div>
                    </section>
				</section>
			</div>
		</xpath>
	</template>
</odoo>