<?xml version="1.0" encoding="UTF-8"?>
<odoo>

	<!-- dynamic config brand templates -->

	<template id="dynamic_config_product_brand_style_1_grid" name="dynamic config product brand style 1 grid">
		<div class="row">
			<t t-if="item_per_row == 5">
				<t t-set="biz_col_size_int" t-value="20"/>
			</t>
			<t t-else="">
				<t t-set="biz_col_size" t-value="12 / item_per_row"/>
				<t t-set="biz_col_size_int" t-value="int(biz_col_size)"/>
			</t>
			<t t-foreach="records" t-as="brand">
				<div t-att-class="'col-lg-%s'%(biz_col_size_int)+' mb-3 col-md-6'">
					<div class="brand_box style_1 h-100 d-flex flex-column justify-content-center">
						<div class="image text-center">
							<a t-attf-href="/shop?brand=#{brand.id}">
								<img class="img img-fluid" t-attf-src="/web/image/product.brand/#{brand.id}/image"/>
							</a>
						</div>
						<!-- <div class="info">
							<h6 class="mb-0"><t t-out="brand.name"/></h6>
						</div> -->
					</div>
				</div>
			</t>
		</div>
	</template>

